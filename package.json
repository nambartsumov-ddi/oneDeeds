{
  "name": "one-deeds",
  "version": "1.0.0",
  "description": "oneDeeds client app",
  "main": "build/server.js",
  "repository": "https://github.com/yotamelkaslasy/oneDeeds",
  "author": "Yotam Elkaslasy",
  "license": "MIT",
  "engines": {
    "node": "10.13.0"
  },
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env CLIENT=true webpack-dev-server --hot --inline",
    "dev:server": "npm-run-all --parallel dev:server:watch dev:server:restart",
    "dev:server:restart": "nodemon --require dotenv/config build/server.js",
    "dev:server:watch": "webpack --watch",
    "build": "npm-run-all --sequential clean build:*",
    "build:client": "cross-env CLIENT=true webpack --progress",
    "build:server": "webpack --progress",
    "clean": "rimraf build",
    "clean:client": "rimraf build/public",
    "clean:server": "rimraf build/*.js build/*.map",
    "deploy": "npm-run-all build deploy:*",
    "deploy:client": "npm-run-all --sequential build:client deploy:cloudstorage setmeta",
    "deploy:server": "gcloud app deploy",
    "deploy:cloudstorage": "gsutil -m rsync -R build/public gs://www.onedeeds.com/",
    "setmeta": "gsutil -m setmeta -h \"Cache-Control:public, max-age=5256000\" gs://www.onedeeds.com/**/*.*",
    "setdefaultpublic": "gsutil -m defacl set public-read gs://www.onedeeds.com/",
    "debug": "ndb .",
    "lint": ""
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.5.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.5.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "helmet": "^3.15.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "jsonwebtoken": "^8.3.0",
    "mailchimp-api-v3": "^1.12.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mongoose": "^5.3.7",
    "morgan": "^1.9.1",
    "node-sass": "^4.10.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "randomstring": "^1.1.5",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-ga": "^2.5.3",
    "react-hot-loader": "^4.3.12",
    "react-loading": "^2.0.3",
    "react-pose": "^4.0.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-stripe-elements": "^2.0.1",
    "react-youtube": "^7.8.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.9",
    "stripe": "^6.12.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "validator": "^10.9.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "ndb": "^1.0.26",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2",
    "prettier-eslint": "^8.8.2",
    "webpack-dev-server": "^3.1.10"
  },
  "browserslist": "last 2 versions, > 0.5%, not dead",
  "postcss": {
    "ident": "postcss",
    "sourceMap": true,
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  }
}

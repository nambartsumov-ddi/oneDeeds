{
  "name": "one-deeds",
  "version": "1.0.0",
  "description": "oneDeeds client app",
  "main": "build/server.js",
  "repository": "https://github.com/yotamelkaslasy/oneDeeds",
  "author": "Yotam Elkaslasy",
  "license": "MIT",
  "engines": {
    "node": "8.11.3"
  },
  "private": true,
  "scripts": {
    "postinstall": "npm-run-all --sequential clean build:server",
    "start": "pm2 start -i 0 build/server.js && pm2 monit",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env CLIENT=true webpack-serve",
    "dev:server": "npm-run-all --parallel dev:server:*",
    "dev:server:restart": "nodemon --require dotenv/config build/server.js",
    "dev:server:watch": "webpack --watch",
    "build": "npm-run-all --sequential clean build:*",
    "build:client": "cross-env CLIENT=true webpack --progress",
    "build:server": "webpack --progress",
    "clean": "rimraf build",
    "clean:client": "rimraf build/public",
    "clean:server": "rimraf build/*.js build/*.map",
    "deploy": "",
    "postdeploy": "",
    "debug": "ndb yarn dev:server",
    "debug:prod": "ndb build/server.js",
    "browser-sync": "browser-sync start --server \"build/public\" --port 3001"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "connected-react-router": "^4.4.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "pm2": "^3.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.1",
    "browser-sync": "^2.24.7",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "husky": "^0.14.3",
    "koa-connect": "^2.0.1",
    "mini-css-extract-plugin": "^0.4.2",
    "ndb": "^1.0.24",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^2.0.2"
  },
  "browserslist": "> 5%, not ie 11, not op_mini all, not dead",
  "postcss": {
    "ident": "postcss",
    "sourceMap": true,
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  }
}

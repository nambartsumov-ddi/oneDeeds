{
  "name": "one-deeds",
  "version": "1.0.0",
  "description": "oneDeeds client app",
  "main": "build/server.js",
  "repository": "https://github.com/yotamelkaslasy/oneDeeds",
  "author": "Yotam Elkaslasy",
  "license": "MIT",
  "engines": {
    "node": "8.11.3"
  },
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env CLIENT=true webpack-dev-server --hot --inline",
    "dev:server": "npm-run-all --parallel dev:server:watch dev:server:restart",
    "dev:server:restart": "nodemon --require dotenv/config build/server.js",
    "dev:server:watch": "webpack --watch",
    "build": "npm-run-all --sequential clean build:*",
    "build:client": "cross-env CLIENT=true webpack --progress",
    "build:server": "webpack --progress",
    "clean": "rimraf build",
    "clean:client": "rimraf build/public",
    "clean:server": "rimraf build/*.js build/*.map",
    "deploy": "npm-run-all deploy:*",
    "deploy:client": "gsutil -m rsync -R build/public gs://www.onedeeds.com/",
    "deploy:server": "gcloud app deploy",
    "setmeta": "gsutil -m setmeta -h \"Cache-Control:public, max-age=5256000\" gs://www.onedeeds.com/**/*.*",
    "setdefaultpublic": "gsutil -m defacl set public-read gs://www.onedeeds.com/",
    "debug": "ndb .",
    "lint": ""
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "autoprefixer": "^9.1.5",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "connect-mongo": "^2.0.1",
    "connected-react-router": "^4.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-loader": "^2.0.0",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.1",
    "image-webpack-loader": "^4.3.1",
    "lodash-es": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.3",
    "moment": "^2.22.2",
    "mongoose": "^5.3.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^3.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "randomstring": "^1.1.5",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-youtube": "^7.7.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "ndb": "^1.0.26",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "webpack-dev-server": "^3.1.9"
  },
  "browserslist": "last 2 versions",
  "postcss": {
    "ident": "postcss",
    "sourceMap": true,
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  }
}
